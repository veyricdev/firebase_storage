{
  "name": "firebase_storage",
  "version": "1.0.0",
  "description": "Build Cloud With Express + Firebase Storage",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec babel-node src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/index.js",
    "lint": "eslint ./src --fix --max-warnings=0",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,css,less,scss,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avnendv/firebase_storage.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "structure"
  ],
  "author": "AVNENDV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avnendv/firebase_storage/issues"
  },
  "homepage": "https://github.com/avnendv/firebase_storage#readme",
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "express-rate-limit": "^7.0.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
